//   27. Display Some sequence.


#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	int n1=1,n2=2,n3=3,n4=4,n5=5;
	cout<<"Number \tSquare \tCube" <<endl<<n1<<"\t"<<n1<<"\t"<<n1<<endl<<n2<<"\t"
	<<n2*n2<<"\t"<<n2*n2*n2<<endl<<n3<<"\t"<<n3*n3<<"\t"<<n3*n3*n3<<endl
	<<n4<<"\t"<<n4*n4<<"\t"<<n4*n4*n4<<endl<<n5<<"\t"<<n5*n5<<"\t"<<n5*n5*n5<<endl;	}
