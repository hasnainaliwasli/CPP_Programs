//    14.Calculate Volume of Cube where   V = length * width * height



#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	
	int l,h,w,V;
	cout<<"Enter Length: ";
	cin>>l;
	cout<<"Enter Width: ";
	cin>>w;
	cout<<"Enter Height: ";
	cin>>h;
	V = h*l*h;
	cout<<"Volume = "<<V;
}
