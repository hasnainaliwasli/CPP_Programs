//   15.Input x and y coordinates and calculate Distance=SQRT((x2 - x1)^2 + (y2 -y1)^2)



#include <iostream>
#include <conio.h>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
	int x1,x2,y1,y2;
	float D;	
	cout<<"Enter x1 coordinate : ";
	cin>>x1;
	cout<<"Enter x2 coordinate : ";
	cin>>x2;
	cout<<"Enter y1 coordinate : ";
	cin>>y1;
	cout<<"Enter y2 coordinate : ";
	cin>>y2;
	int a = (x2-x1);
	int b = (y2-y1);
	D = sqrt (a*a+b*b);
	cout<<"Distance= "<<setprecision(4)<<D;
}
