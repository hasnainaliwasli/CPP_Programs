#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	int a,b,c,d,e,sum;
	cout<<"Enter 1st digit: ";
	cin>>a;
	cout<<"Enter 2nd digit: ";
	cin>>b;
	cout<<"Enter 3rd digit: ";
	cin>>c;
	cout<<"Enter 4th digit: ";
	cin>>d;	
	cout<<"Enter 5th digit: ";
	cin>>e;	
	sum = a+b+c+d+e;	
	cout<<"Sum = "<<sum;	
}



/*    23.input Basic Salary  calculate 35% dearness allowence, 25% house rent displaty gross
salary.Gross salary = Basic salary + HRA (House rent allowance) + DA (dearness allowance) */
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	int BS,DA,HR,GS;
	cout<<"Enter Basic salary: ";
	cin>>BS;
	DA = BS * 35 /100;
	HR = BS * 25 / 100;
	GS = BS + DA + HR;
	cout<<"Gross Salary = "<<GS;
}


//    24.Input two times in hh:mm:ss and add them and display total time.
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
int hh1, hh2, mm1, mm2, ss1, ss2, hh=0, mm=0, ss=0;
    cout<<"Enter time 1 in hh:mm:ss format : ";
    cin>>hh1>>mm1>>ss1;  
    cout<<"Enter time 2 in hh:mm:ss format : ";
    cin>>hh2>>mm2>>ss2;
    ss = ss1 + ss2;
    mm = ss / 60;
    ss = ss % 60;
    mm = mm + (mm1 + mm2);
    hh = mm / 60;
    mm = mm % 60;
    hh = hh + (hh1 + hh2);
    cout<<"Sum of two times is : "<<hh<<":"<<mm<<":"<<ss<<endl;	
}



//    25.   Input principal, rate, time and Display Compound Interest.
#include <iostream>
#include <conio.h>
#include <cmath>
using namespace std;
int main()
{
    float principal, rate, time, result;
    cout << "Enter principal : ";
    cin >> principal;
    cout << "Enter rate : ";
    cin >> rate;
    cout <<"Enter time (year) : ";
    cin >> time;
    result = principal * pow((1 + rate / 100), time);
    cout << endl << "Compound interest : " << result;	
}


