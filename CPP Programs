
//  1 .Write program to print Text,Character,Integer and Floating Values.

#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	cout<<"Text : My name is Hasnain Ali"<<endl<<"Integer Values: 1,2,3,4,5,6,7,8,9"<<endl
	<<"Special Characters: !@#$%^&*()"<<endl<<"Floating Point Values: 1.2,1.3,1.4,1.5,1.6,1.7"<<endl;
}

// 2.Input radius and calculate Area=4(PI)(R^2)  and Circumference= 4/3(PI)(R^3) where PI=3.14

#include <iostream>
#include<conio.h>
#define PI 3.14
using namespace std;
int main(){
int R,A,C;

	cout<<"Enter the Radius: ";
	cin>>R;
	A = 4*PI*R*R;
	C = 4/3*PI*R*R*R;
	cout<<"Area = "<<A<<endl;
	cout<<"Circumference = "<<C<<endl;
}


//  3. Get 3 sides a,b,c of Triangle.Find Area=SQRT(s(s-a)(s-b)(s-c) where s=(a+b+c)/2

#include <iostream>
#include <conio.h>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
	float a,b,c,s,Area;
	cout<<"Enter 1st side : ";
	cin>>a;
	cout<<"Enter 2nd side : ";
	cin>>b;
	cout<<"Enter 3rd side : ";
	cin>>c;
	s= (a+b+c)/2;
	Area = sqrt(s*(s-a)*(s-b)*(s-c));
	cout<<"Area = "<<setprecision(3)<<Area<<endl;	
}



//   4.Input miles and print Kilometers  where  1 Mile = 1.609 KM  .
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{float miles,KM;
	cout<<"Enter the Miles: ";
	cin>>miles;
	KM = 1.609 * miles;
	cout<<"Kilometers =  "<<KM;	
}

// 5. Input 4 numbers and print Sum,Product and Average.

#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	float a,b,c,d,product,sum,average;
	cout<<"Enter the 1st Number: ";
	cin>>a;
	cout<<"Enter the 2nd Number: ";
	cin>>b;
	cout<<"Enter the 3rd Number: ";
	cin>>c;
	sum = a+b+c;
	product = a*b*c;
	average = sum/4;
	cout<<"Sum = "<<sum<<endl;
	cout<<"Product = "<<product<<endl;
	cout<<"Average = "<<average<<endl;	
}




//  6. Input age in Years and display in months and days.
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	float years,months;
	int days;
	cout<<"Enter your age in Years: ";
	cin>>years;
	months = years * 12;
	days = years*365;
	cout<<"Age in Months: "<<months<<endl;
	cout<<"Age in Days: "<<days<<endl;
}



//  7.input a Number and display Square and Cube.
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	float n,square,cube;
	cout<<"Enter a Number: ";
	cin>>n;
	square = n*n;
	cube = n*n*n;
	cout<<"Square is : "<<square<<endl;
	cout<<"Cube is : "<<cube<<endl;	
}



//   8. Input total pages of book,Person read in one day,Number of days read and Display How many pages read and how many remaining.
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	int pages,reads,days,readed,remaining;
	cout<<"Enter the total Pages of Book:  ";
	cin>>pages;
	cout<<"How many pages a Person read in a Day: ";
	cin>>reads;
	cout<<"How many Days he reads: ";
	cin>>days;
	readed = reads * days;
	remaining = pages - readed;
	if(readed > pages){
	cout<<"The Book is finished,He can read "<< readed <<" Pages in "<< days <<" days";
	}
	else
	{	
	cout<<"Pages Readed: "<<readed<<endl<<"Remaining Pages are : "<<remaining;
}	
}


//   9. Car can Travel 5.3 miles in 1 litter. Input petrol and tell distance.
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	float P,D;
	cout<<"Enter the Petrol in liters: ";
	cin>>P;
	
	D = P * 5.3;
	cout<<"Car will Cover "<<D<<" Miles in "<<P<<" Liter Petrol";	
}


//  10. Input total number of students and fee per student .Display total collected fee.



#include <iostream>
#include <conio.h>
using namespace std;
int main()
{int n;
	cout<<"Enter number of students: ";
	cin>>n;
	int Fee[n];
	int sum = 0;
	for (int i=0;i<n;i++){	
		cout<<"Enter "<<i+1<<" Student Fee: ";
		cin>>Fee[i];
		sum = sum + Fee[i];
	}
	cout<<"Total fees collected from class is : "<<sum;
}


// 11.Convert Fahrenheit to Celsius  where  C=5/9(F-32)
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{float F,C;
	cout<<"Enter the temperature in Fahrenheit: ";
	cin>>F; 
	C=(F - 32.0) * 5.0 / 9.0;
	cout<<"Temperature in Celsius is: "<<C;
}



