#include <conio.h>
using namespace std;
int main()
{
	char ch;
    cout<<"Enter a character : ";
    cin>>ch;
    cout<<"Next two characters : ";
    ch++;
    cout<<ch<<"\t , ";
    ch++;
    cout<<ch<<"\t";
}



