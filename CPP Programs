//    14.Calculate Volume of Cube where   V = length * width * height
#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	
	int l,h,w,V;
	cout<<"Enter Length: ";
	cin>>l;
	cout<<"Enter Width: ";
	cin>>w;
	cout<<"Enter Height: ";
	cin>>h;
	V = h*l*h;
	cout<<"Volume = "<<V;
}



//   15.Input x and y coordinates and calculate Distance=SQRT((x2 - x1)^2 + (y2 -y1)^2)



#include <iostream>
#include <conio.h>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
	int x1,x2,y1,y2;
	float D;	
	cout<<"Enter x1 coordinate : ";
	cin>>x1;
	cout<<"Enter x2 coordinate : ";
	cin>>x2;
	cout<<"Enter y1 coordinate : ";
	cin>>y1;
	cout<<"Enter y2 coordinate : ";
	cin>>y2;
	int a = (x2-x1);
	int b = (y2-y1);
	D = sqrt (a*a+b*b);
	cout<<"Distance= "<<setprecision(4)<<D;
}



//   16.Swap values of three variables using 4th variable.



#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
	int a,b,c,temp;
	cout<<"Enter a variable: ";
	cin>>a;
	cout<<"Enter b variable: ";
	cin>>b;
	cout<<"Enter c variable: ";
	cin>>c;
	temp=a;
	a=b;
	b=c;
	c=temp;
	cout<<"values after Swapping a,b and c Variable"<<endl;
	cout<<"a= "<<a<<endl<<"b= "<<b<<endl<<"c= "<<c;	
}


