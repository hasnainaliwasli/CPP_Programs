#include <iostream>
#include <conio.h>
using namespace std;

int Table(int,int);

int main(){
	int a,b;
	cout<<"Enter the number of table you want to print: ";
	cin>>a;
	
	cout<<"How much terms you want to calculate: ";
	cin>>b;

   Table(a,b);


}

int Table(int a,int b){
	

	
	for(int i=1 ; i<=b ; i++)
	{
		 
		cout<<i<<"*"<<a<<"="<<i*a<<endl;
		
	}
	
}
